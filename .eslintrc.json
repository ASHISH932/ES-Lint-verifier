{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": "off",
        "indent": [
            "error",
            2
        ],
        "generator-star": "off",
        "global-strict": "off",
        "newline-before-return": "error",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "lines-around-directive": ["error", "unix"],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
